FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 153.1;

boundaryField
{
    inlet
    {
        type            totalTemperature;
        value           uniform 153.1;
        T0              uniform 344.44;
        U               U;
        phi             phi;
        psi		thermo:psi;
        gamma           1.4;
        
    }
    outlet
    {
        type            zeroGradient;
    }
    upperWall
    {
        type           zeroGradient;
    }
    lowerWall
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}



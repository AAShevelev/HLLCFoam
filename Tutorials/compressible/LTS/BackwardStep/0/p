FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 13316.6;

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }

    inlet
    {
        type            totalPressure;
        value           uniform 227527;
        p0              uniform 227527;
        U               U;
        phi             phi;
        psi		thermo:psi;
        gamma           1.4;
    }

    upperWall
    {
        type            zeroGradient;
    }

    lowerWall
    {
        type            zeroGradient;
    }

    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //

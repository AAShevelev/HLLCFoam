FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0.021;

boundaryField
{
    inlet
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength	0.09;
        value           uniform 0.021;
    }
    outlet
    {
        type            zeroGradient;
    }
    upperWall
    {
        type            omegaWallFunction;
        value           uniform 0.021;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 14.855;
    }
    lowerWall
    {
        type            omegaWallFunction;
        value           uniform 0.021;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 14.855;
    }
    frontAndBack
    {
        type            empty;
    }
}



FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            uniformFixedValue;
        uniformValue    constant (1500 0 0);
        value           $internalField;
    }
    outlet
    {
        type            zeroGradient;
    }
    cone_fluid
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    #include "wedgeAndSlip"
}

